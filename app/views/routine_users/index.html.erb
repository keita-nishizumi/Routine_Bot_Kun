<div class="contents row routine_index">
  <h1 class="grid-6 page_title">Today's routines</h1>
  <table border cellpadding="4 8" class="index_table">
    <tr>
      <th class="cell">Time</th>
      <th class="cell">Routine</th>
      <th class="cell">CheckLists</th>
    </tr>
    <% @routine_users.each do |routine_user| %>
      <%= render partial: "routine", locals: { routine_user: routine_user } %>
    <% end %>
  </table>
</div>